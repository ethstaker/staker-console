{
  "name": "staker-dashboard",
  "version": "0.1.0",
  "type": "module",
  "description": "Staker Dashboard",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "serve -s dist",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint --max-warnings=0 \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts:check": "tsc --noEmit",
    "ts:check:strict": "tsc --noEmit --skipLibCheck",
    "ts:check:staged": "node scripts/ts-check-staged.js",
    "test:unit": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chainsafe/ssz": "^1.2.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^7.3.5",
    "@reown/appkit": "^1.7.13",
    "@reown/appkit-adapter-wagmi": "^1.7.13",
    "@tanstack/react-query": "^5.83.0",
    "bignumber.js": "^9.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.3",
    "react-router-dom": "^6.24.1",
    "recoil": "^0.7.7",
    "serve": "^14.1.2",
    "wagmi": "^2.15.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "eslint": "^9.39.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.41",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.10",
    "tsc-files": "^1.1.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "node scripts/ts-check-staged.js",
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json,css,scss,md}": "prettier --write"
  },
  "keywords": []
}
